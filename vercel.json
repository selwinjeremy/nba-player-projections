{
    "builds": [
      {
        "src": "nbaprojections/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "functions": {
        "nbaprojections/wsgi.py": {
            "maxDuration": 60
        }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "nbaprojections/wsgi.py"
      }
    ]
}